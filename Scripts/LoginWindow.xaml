        <Label x:Name="lblInfo" Content="PC à accéder (Adresse IP ou Nom du PC) :" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbName" HorizontalAlignment="Left" Height="23" Margin="245,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="223"/>
        <Button x:Name="btnConnect" Content="Connexion" HorizontalAlignment="Left" Margin="383,55,0,0" VerticalAlignment="Top" Width="85" Height="25"/>
    </Grid>
</Window>
